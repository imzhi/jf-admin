{
    "name": "imzhi/jf-admin",
    "type": "library",
    "description": "jf-admin 是基于 Laravel 的后台管理模板，提供后台登录、权限控制和操作日志查看等功能，前端框架使用 inspinia。",
    "license": "MIT",
    "authors": [
        {
            "name": "imzhi",
            "email": "yxz_blue@126.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "mews/captcha": "^2.2|~3.0",
        "spatie/laravel-activitylog": "^2.8|~3.0",
        "spatie/laravel-permission": "^2.37|~3.0",
        "doctrine/annotations": "^1.8",
        "laravel/framework": "~5.5"
    },
    "require-dev": {
        "laravel/laravel": "~5.5"
    },
    "autoload": {
        "psr-4": {
            "Imzhi\\JFAdmin\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Imzhi\\JFAdmin\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Imzhi\\JFAdmin\\JFAdminServiceProvider"
            ],
            "aliases": {
                "JFAdmin": "Imzhi\\JFAdmin\\Facades\\JFAdmin"
            }

        }
    },
    "minimum-stability": "dev"
}
